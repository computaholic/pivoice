# sample dict

[playmusic]
	ListenFor 	= play music
	Action		= echo "<play music>"

[playsong]
	ListenFor 	=~ play $_song from $_band
	Action 		= echo "vlc /path/to/music/$_band/$_song.mp3"

[playvideo]
	ListenFor 	= ^play\b(.*?)\bseason (.*?) episode (.*?)
	# default: simple
	MatchStyle	= regex
	Action 		= echo "mplayer /videos/$1/season_$2/episode_$3"
	
[saytest]
	ListenFor 	= say $_INPUT
	Action 		= !say $_INPUT
	
[default]
	MatchStyle 	= simple
	Action 		= echo "NONE"
