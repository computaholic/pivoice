# sample dict

[playmusic]
	ListenFor 	= play music
	Action		= echo "<play music>"

[playsong]
	ListenFor 	=~ play $song from $band
	Action 		= echo "vlc /path/to/music/$band/$song.mp3"

[playvideo]
	ListenFor 	= ^play\b(.*?)\bseason (.*?) episode (.*?)
	# default: simple
	MatchStyle	= regex
	Action 		= echo "mplayer /videos/$1/season_$2/episode_$3"
	
[default]
	MatchStyle 	= simple
	Action 		= echo "NONE"
